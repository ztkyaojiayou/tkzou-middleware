<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	         http://www.springframework.org/schema/beans/spring-beans.xsd
		 http://www.springframework.org/schema/context
		 http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="person" class="com.tkzou.middleware.springframework.test.ioc.bean.Person">
        <property name="name" value="tkzou"/>
        <property name="car" ref="car"/>
    </bean>

    <bean id="car" class="com.tkzou.middleware.springframework.test.ioc.bean.Car">
        <property name="brand" value="porsche"/>
    </bean>
    <bean class="com.tkzou.middleware.springframework.test.ioc.common.CustomBeanFactoryPostProcessor"/>

    <bean class="com.tkzou.middleware.springframework.test.ioc.common.CustomerBeanPostProcessor"/>

</beans>